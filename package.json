{
  "name": "exile-diary",
  "version": "0.3.20",
  "main": "build/main/index.js",
  "build": {
    "appId": "com.pathofexile.exilediary",
    "productName": "Exile Diary",
    "win": {
      "target": "nsis",
      "artifactName": "exile-diary-setup-${version}.${ext}",
      "icon": "./res/img/icons/win/ExileDiary.ico",
      "extraResources": [
        "eng.traineddata"
      ]
    },
    "nsis": {
      "oneClick": "false",
      "allowElevation": "false",
      "allowToChangeInstallationDirectory": "true",
      "installerIcon": "./res/img/icons/win/ExileDiary.ico",
      "uninstallerIcon": "./res/img/icons/win/ExileDiary.ico",
      "installerHeaderIcon": "./res/img/icons/win/ExileDiary.ico"
    },
    "linux": {
      "target": "deb",
      "executableName": "Exile-Diary",
      "icon": "./res/img/icons/png",
      "category": "Utility",
      "maintainer": "briansd9"
    }
  },
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "pretty": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run react:start\" \"wait-on http://localhost:3000 && tsc -p main -w\" \"wait-on http://localhost:3000 && tsc -p main && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "author": "joshi",
  "repository": {
    "type": "git",
    "url": "https://github.com/qt-dev/exile-diary.git"
  },
  "contributors": [],
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "color-convert": "^2.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-overlay-window": "^2.0.1",
    "electron-updater": "^5.2.1",
    "fast-equals": "^4.0.1",
    "imgur": "^2.2.0",
    "jimp": "^0.16.1",
    "js-levenshtein": "^1.1.6",
    "merge-img": "^2.1.3",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "nodejs-tail": "^1.1.1",
    "opn": "^6.0.0",
    "pastebin-js": "^1.0.6",
    "ps-list": "^7.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "request": "^2.88.2",
    "sqlite3": "^5.0.11",
    "tesseract.js": "^3.0.2",
    "winston": "^3.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "electron-reload": "^2.0.0-alpha.1",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "tslib": "^2.5.0",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  }
}
